{
  "name": "medium-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cloudinary": "^1.11.0",
    "compression": "^1.7.2",
    "connect-multiparty": "^2.1.0",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "medium-editor": "^5.23.3",
    "medium-editor-insert-plugin": "^2.5.0",
    "mongoose": "^5.0.14",
    "prettier": "^1.12.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-google-login": "^3.2.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.4",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "lint-staged": {
    "*/**/*.{js, jsx, json}": [
      "eslint --fix",
      "prettier --config .prettierrc --write"
    ],
    "src/**/*.{scss,css}": [
        "stylelint --config=.stylelintrc --fix",
        "prettier --config .prettierrc --write"
  ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "dev": "nodemon -w server/app.js -d \"npm start\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "test:lint:js": "eslint --ext=js --ext=jsx .",
    "test:lint:scss": "stylelint --config=.stylelintrc '**/*.css'",
    "test:lint": "run-s test:lint:**",
    "test:unit": "react-scripts test --env=jsdom",
    "test": "run-s test:**",
    "lint": "node_modules/.bin/eslint --ext=js --ext=jsx --fix .",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettify": "./node_modules/.bin/prettier --config .prettierrc --write '*/**/*.{js,jsx,css}'"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0"
  }
}
